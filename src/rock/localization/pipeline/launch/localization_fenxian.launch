<launch>
    <node pkg="localization_pipeline" type="localization" name="localization" output="screen">
      <param name="command_no_gps" type="bool" value = "false"/>
      <param name="localization_debug" type="bool" value = "true"/>
      <param name="slam_err_fix" type="double" value = "0.002"/>
      <param name="slam_err_normal" type="double" value = "0.002"/>
      <param name="slam_yaw_err_fix" type="double" value = "0.004"/>
      <param name="slam_yaw_err_normal" type="double" value = "0.004"/>
      <param name="slam_fix_param" type="double" value = "0.15"/>
      <param name="slam_error_param" type="double" value = "0.1"/>
      <param name="speed_err" type="double" value = "0.1"/>
      <param name="imu_err" type="double" value = "0.001"/>
    </node>
</launch>
